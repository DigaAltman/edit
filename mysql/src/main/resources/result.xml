<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="*">
    <resultMap id="course" type="cn.bbzzzs.db.test.pojo.Course">
        <id column="course_id" property="courseId"/>
        <result column="course_name" property="courseName"/>
        <result column="course_address" property="courseAddress"/>
        <collection property="studentList" ref="student"/>
    </resultMap>

    <resultMap id="student" type="cn.bbzzzs.db.test.pojo.Student">
        <id column="student_id" property="studentId"/>
        <result column="student_name" property="studentName"/>
        <result column="student_sex" property="studentSex"/>
        <result column="student_address" property="studentAddress"/>
        <association property="course" ref="course"/>
    </resultMap>
</mapper>
